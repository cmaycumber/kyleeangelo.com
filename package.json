{
  "private": true,
  "name": "emilia",
  "description": "Minimalistic portfolio/photography site with masonry grid, page transitions and big images. Themeable with Theme UI. Includes Light/Dark mode.",
  "version": "0.2.2",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "MIT",
  "starter-name": "gatsby-starter-portfolio-emilia",
  "scripts": {
    "remove:theme:gatsby-node": "rm -f node_modules/@lekoarts/gatsby-theme-emilia/gatsby-node.js",
    "build": "yarn remove:theme:gatsby-node && gatsby build",
    "develop": "yarn remove:theme:gatsby-node && gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-emilia": "^1.3.17",
    "@theme-ui/presets": "^0.3.0",
    "gatsby": "^2.24.85",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-netlify": "^2.3.24",
    "gatsby-plugin-offline": "^3.2.37",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-typescript": "^2.4.24",
    "gatsby-source-datocms": "^2.5.7",
    "react": "16.13.x",
    "react-dom": "16.13.x",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@types/theme-ui": "^0.3.7",
    "cross-env": "^7.0.2"
  }
}
